From 7d43caa9fa4d6257dbe75d5be3232327cb60633c Mon Sep 17 00:00:00 2001
From: "Cui, Yuxin" <yuxin.cui@intel.com>
Date: Tue, 12 Jul 2022 11:11:18 +0800
Subject: [PATCH] Enable AIDL Security Keymint

Enable the following AIDLs:
IKeyMintDevice (@2)
IRemotelyProvisionedComponent (@2)

Tracked-On: OAM-102187
Signed-off-by: Cui, Yuxin <yuxin.cui@intel.com>
Change-Id: Ie11e7bab0b44a4052821920d48c749f31120e0b4
---
 groups/device-specific/caas/framework_manifest.xml | 3 ++-
 groups/device-specific/caas/manifest.xml           | 3 ++-
 2 files changed, 4 insertions(+), 2 deletions(-)

diff --git a/groups/device-specific/caas/framework_manifest.xml b/groups/device-specific/caas/framework_manifest.xml
index 17fd2d4..3c12bf8 100644
--- a/groups/device-specific/caas/framework_manifest.xml
+++ b/groups/device-specific/caas/framework_manifest.xml
@@ -6,7 +6,7 @@
 {{/ota-update}}
     <hal format="aidl">
         <name>android.hardware.security.keymint</name>
-        <version>1</version>
+        <version>2</version>
         <interface>
             <name>IKeyMintDevice</name>
             <instance>default</instance>
@@ -14,6 +14,7 @@
     </hal>
     <hal format="aidl">
         <name>android.hardware.security.keymint</name>
+        <version>2</version>
         <interface>
             <name>IRemotelyProvisionedComponent</name>
             <instance>default</instance>
diff --git a/groups/device-specific/caas/manifest.xml b/groups/device-specific/caas/manifest.xml
index 0efa515..a0fafa9 100644
--- a/groups/device-specific/caas/manifest.xml
+++ b/groups/device-specific/caas/manifest.xml
@@ -6,7 +6,7 @@
 {{/ota-update}}
     <hal format="aidl">
         <name>android.hardware.security.keymint</name>
-        <version>1</version>
+        <version>2</version>
         <interface>
             <name>IKeyMintDevice</name>
             <instance>default</instance>
@@ -14,6 +14,7 @@
     </hal>
     <hal format="aidl">
         <name>android.hardware.security.keymint</name>
+        <version>2</version>
         <interface>
             <name>IRemotelyProvisionedComponent</name>
             <instance>default</instance>
