From b967b29e0c34731028c59c08f69b99dd3ccd396c Mon Sep 17 00:00:00 2001
From: ahs <amrita.h.s@intel.com>
Date: Tue, 17 Mar 2020 13:09:09 +0530
Subject: Update LOCAL_MODULE_CLASS to SCRIPT in bt & wlan

Tracked-On:
Signed-off-by: ahs <amrita.h.s@intel.com>
---
 bluetooth/rfkill/Android.mk | 2 +-
 wlan/Android.mk             | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/bluetooth/rfkill/Android.mk b/bluetooth/rfkill/Android.mk
index 6c56067..625b489 100644
--- a/bluetooth/rfkill/Android.mk
+++ b/bluetooth/rfkill/Android.mk
@@ -5,7 +5,7 @@ LOCAL_PATH := $(my-dir)
 include $(CLEAR_VARS)
 LOCAL_MODULE := rfkill_bt.sh
 LOCAL_MODULE_TAGS := optional
-LOCAL_MODULE_CLASS := EXECUTABLE
+LOCAL_MODULE_CLASS := SCRIPT
 LOCAL_MODULE_PATH := $(TARGET_OUT_EXECUTABLES)
 LOCAL_SRC_FILES := rfkill_bt.sh
 include $(BUILD_PREBUILT)
diff --git a/wlan/Android.mk b/wlan/Android.mk
index 585c87f..c72733f 100644
--- a/wlan/Android.mk
+++ b/wlan/Android.mk
@@ -3,7 +3,7 @@ LOCAL_PATH := $(call my-dir)
 include $(CLEAR_VARS)
 LOCAL_MODULE := dhcp_lease_evt.sh
 LOCAL_MODULE_TAGS := optional
-LOCAL_MODULE_CLASS := SYSTEM
+LOCAL_MODULE_CLASS := SCRIPT
 LOCAL_MODULE_PATH := $(TARGET_OUT_EXECUTABLES)
 LOCAL_SRC_FILES := $(LOCAL_MODULE)
 include $(BUILD_PREBUILT)
